{
  "name": "org-checkbox",
  "displayName": "org-checkbox",
  "description": "Adds coloring for bullet-journal-style checkbox items to org-mode files",
  "version": "0.0.1",
  "publisher": "publicus",
  "repository": "https://github.com/publicus/vscode-language-org-checkboxes",
  "license": "BSD-3",
  "engines": {
    "vscode": "^1.38.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "grammars": [
      {
        "scopeName": "source.checkboxes.org",
        "path": "./syntaxes/org-checkboxes.tmLanguage.json",
        "injectTo": [
          "source.org"
        ]
      },
      {
        "scopeName": "source.checkboxes.org",
        "path": "./syntaxes/org-checkboxes.tmLanguage.json",
        "injectTo": [
          "text.html.markdown"
        ]
      }
    ],
    "themes": [
      {
        "label": "Org-mode Checkbox Highlighting",
        "uiTheme": "vs",
        "path": "./solarized-light-color-theme.json"
      }
    ]
  }
}
